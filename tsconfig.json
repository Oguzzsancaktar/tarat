{
  "compilerOptions": {
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "interfaces/*": [
        "./packages/interfaces/*"
      ],
      "common/*": [
        "./packages/common/*"
      ],
      "ui/*": [
        "./packages/ui/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/types",
    "apps/next/out",
    "apps/next/.next",
    "apps/next/.tamagui"
  ]
}
