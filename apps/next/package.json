    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.5",
    "react-native-web": "~0.19.6",
    "vercel": "latest"
  },
  "devDependencies": {
    "@tamagui/next-plugin": "latest",
    "@types/node": "^20.8.9",
    "eslint-config-next": "^13.0.4"
